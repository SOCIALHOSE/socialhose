<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title></title>
</head>

{# Common styles #}
{%- set tableStyles = 'border="0" cellspacing="0" cellpadding="0"' -%}
{%- set tableStylesFullWidth = 'width="100%" '~tableStyles -%}

{%- autoescape false -%}
<body style='padding:0; margin:0;'>
	<table {{ tableStylesFullWidth|raw }}>
		<tr>
			<td align='center' bgcolor='#eaeaea'>
				<table width='728' {{ tableStyles|raw }} bgcolor="#ffffff">
					<tr>
						<td align='center'>
							{# Body #}
							<table {{ tableStylesFullWidth|raw }}>
                                {%- block body -%}
                                    {{- body|raw -}}
                                {%- endblock body -%}
							</table>
						</td>
					</tr>
				</table>
			</td>
		</tr>
	</table>
</body>
{%- endautoescape -%}
</html>